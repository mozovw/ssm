					rest=#{item.rest},
				</if>
			</set>
			<where>
				<if test="item.id!=null and item.id!=''">
					and id=#{item.id}
				</if>
				<if test="item.name!=null and item.name!=''">
					and name=#{item.name}
				</if>
				<if test="item.gender!=null and item.gender!=''">
					and gender=#{item.gender}
				</if>
				<if test="item.birth!=null and item.birth!=''">
